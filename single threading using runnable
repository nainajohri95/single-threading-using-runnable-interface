class ABC
{
  void func()
  {
    System.out.println("hello");
  }
}
class rndemo extends ABC implements runnable
{
  public void run()
  {
    for(int i=1;i<=10;i++)
    {
      System.out.println(i);
      try
      {
        Thread.sleep(100);
      }
      catch(Exception e)
      {
        System.out.println(e);
      }
    }
  }
}
